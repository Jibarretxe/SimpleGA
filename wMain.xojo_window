#tag Events pbStart
	#tag Event
		Sub Action()
		  Dim i,j,PopulationSize, NumberOfCrosses, NumberOfClones as Integer
		  
		  ///////////////////////////////////77
		  'Initialize variables
		  
		  if val(txtMutationRate.text)>0 then
		    MutationRate=val(txtMutationRate.text)
		  else
		    msgbox ("First enter all paprmeters")
		  end if
		  
		  if val(txtNumberOfClones.text)>0 then
		    NumberOfClones=val(txtNumberOfClones.text)
		  else
		    msgbox ("First enter all paprmeters")
		  end if
		  
		  if val(txtnumberOfCrosses.text)>0 then
		    NumberOfCrosses=val(txtnumberOfCrosses.text)
		  else
		    msgbox ("First enter all paprmeters")
		  end if
		  
		  if val(txtPopulationSize.text)>0 then
		    PopulationSize=val(txtPopulationSize.text)
		  else
		    msgbox ("First enter all paprmeters")
		  end if
		  
		  if val(txtYRange.text)>0 then
		    RangeY=val(txtYRange.text)
		  else
		    msgbox ("First enter all paprmeters")
		  end if
		  
		  if val(txtXRange.text)>0 then
		    RangeX=val(txtXRange.text)
		  else
		    msgbox ("First enter all paprmeters")
		  end if
		  
		  
		  
		  
		  //////////////////////////
		  'Create a first population
		  
		  For i=1 to PopulationSize
		    Dim NewIndividual as new clsIndividual
		    CurrentPopulation.Append NewIndividual
		  next i
		  
		  
		End Sub
	#tag EndEvent
